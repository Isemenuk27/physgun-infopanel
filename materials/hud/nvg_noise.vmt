"UnlitGeneric"
{
	"$baseTexture" "hud/nvg_noise"
	"$surfaceprop" "Default"
	"$additive" 	1
	"$vertexcolor" 	1
	"$vertexalpha" 	1
	"$ignorez"	1

	"Proxies" { 
       "TextureScroll"
        {
            "textureScrollVar" "$basetexturetransform"
            "textureScrollRate" ".3"
            "textureScrollAngle" "45"
        }

		"AnimatedTexture" { 
			"animatedTextureVar" "$basetexture" 
			"animatedTextureFrameNumVar" "$frame" 
			"animatedTextureFrameRate" "35" 
		}
	} 
}